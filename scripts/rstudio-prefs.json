{
  "panes": {
    "quadrants": ["Source", "TabSet1", "Console", "TabSet2"],
    "tabSet1": [
      "Environment",
      "History",
      "Connections",
      "Build",
      "VCS",
      "Tutorial",
      "Presentation"
    ],
    "tabSet2": [
      "Files",
      "Plots",
      "Packages",
      "Help",
      "Viewer",
      "Presentations"
    ],
    "hiddenTabSet": [],
    "console_left_on_top": false,
    "console_right_on_top": true,
    "additional_source_columns": 0
  },
  "always_save_history": false,
  "auto_append_newline": true,
  "auto_detect_indentation": true,
  "auto_expand_error_tracebacks": true,
  "check_arguments_to_r_function_calls": true,
  "check_unexpected_assignment_in_function_call": true,
  "code_completion_include_already_used": true,
  "color_preview": false,
  "console_double_click_select": true,
  "copilot_enabled": true,
  "copilot_indexing_enabled": true,
  "default_encoding": "UTF-8",
  "default_project_location": "~",
  "default_sweave_engine": "knitr",
  "doc_outline_show": "all",
  "editor_line_height": 125,
  "editor_scroll_multiplier": 50,
  "editor_theme": "Cobalt",
  "font_size_points": 12,
  "full_project_path_in_window_title": true,
  "git_signed_commits": true,
  "global_theme": "alternate",
  "graphics_backend": "ragg",
  "help_font_size_points": 12,
  "highlight_r_function_calls": true,
  "highlight_selected_line": true,
  "indent_guides": "gray",
  "insert_spaces_around_equals": true,
  "jobs_tab_visibility": "shown",
  "load_workspace": false,
  "margin_column": 80,
  "new_proj_git_init": true,
  "new_proj_use_renv": true,
  "num_spaces_for_tab": 2,
  "pdf_previewer": "none",
  "posix_terminal_shell": "bash",
  "rainbow_parentheses": true,
  "reformat_on_save": true,
  "reindent_on_paste": true,
  "remove_history_duplicates": true,
  "reuse_sessions_for_project_links": true,
  "rmd_auto_date": true,
  "rmd_chunk_output_inline": false,
  "save_files_before_build": true,
  "save_workspace": "never",
  "scroll_past_end_of_document": true,
  "server_editor_font": "Fira Code",
  "server_editor_font_enabled": true,
  "shiny_viewer_type": "browser",
  "show_doc_outline_rmd": true,
  "show_help_tooltip_on_idle": true,
  "show_hidden_files": true,
  "show_last_dot_value": true,
  "show_panel_focus_rectangle": true,
  "soft_wrap_r_files": true,
  "source_with_echo": false,
  "strip_trailing_whitespace": true,
  "style_diagnostics": true,
  "syntax_color_console": true,
  "tab_multiline_completion": true,
  "use_tinytex": true,
  "view_dir_after_r_cmd_check": true,
  "warn_if_no_such_variable_in_scope": true,
  "warn_variable_defined_but_not_used": true
}
